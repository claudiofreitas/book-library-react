    "antd": "^4.15.6",
    "dayjs": "^1.8.30",
    "pages": "^0.0.16",
    "parcel-bundler": "^1.12.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cypress": "^7.3.0",
    "eslint": "^7.26.0",
